<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary x:Class="App.Resources.Styles.LightTheme"
                    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                    Source="DefaultTheme.xaml">
    <Color x:Key="BackgroundColour">#fff</Color>
    <Color x:Key="Primary">#6200eee</Color>

    <Color x:Key="Text">#000</Color>

    <Color x:Key="BreadcrumbBackground">#fff</Color>
    <Color x:Key="LastBreadcrumbBackground">#dee4e7</Color>
    <Color x:Key="BreadcrumbTextColour">#757575</Color>
    <Color x:Key="LastBreadcrumbTextColour">#000</Color>
</ResourceDictionary>